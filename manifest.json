{
    "manifest_version":3,
    "name":"Ad blocker",
    "version":"1.0",
    "permissions":["blocking"],
    "background":{
        "service_worker":"background.js"
    },
    "action":{
        "default_popup":"popup.html",
        "default_icon":{
            "16":"icon.png",
            "48":"icon.png",
            "128":"icon.png"
        }
    },
    "icons":{
        "16":"icon.png",
        "48":"icon.png",
        "128":"icon.png"
    },
    "host permissions":[
        "https://*/*",
        "http://*/*"
    ]
}